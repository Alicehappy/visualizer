{
  "name": "ui-router-visualizer",
  "description": "UI-Router State Visualizer and Transition Visualizer",
  "version": "1.1.1",
  "scripts": {
    "example": "webpack-dev-server --config webpack.example.config.js --host 0.0.0.0 --port 8000 --history-api-fallback --content-base example",
    "clean": "rm -rf ./build ./release",
    "compile": "./node_modules/typescript/bin/tsc",
    "bundle-prep": "cp src/*.css build/src/",
    "bundle": "npm run bundle-prep && webpack",
    "all": "npm run clean && npm run compile && npm run bundle",
    "release": "./release.sh"
  },
  "homepage": "http://ui-router.github.com/visualizer",
  "contributors": [
    {
      "name": "Chris Thielen",
      "web": "https://github.com/christopherthielen"
    }
  ],
  "maintainers": [
    {
      "name": "UI-Router",
      "web": "https://github.com/ui-router?tab=members"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-router/visualizer.git"
  },
  "bugs": {
    "url": "https://github.com/ui-router/visualizer/issues"
  },
  "license": "MIT",
  "dependencies": {
    "angular": "^1.4"
  },
  "peerDependencies": {
    "angular-ui-router": "next",
    "d3": "^3.5.14"
  },
  "devDependencies": {
    "angular": "^1.4.9",
    "angular-ui-router": "next",
    "css-loader": "^0.23.1",
    "d3": "^3.5.14",
    "es6-shim": "^0.35.0",
    "oclazyload": "^1.0.9",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.2",
    "typescript": "1.8.0",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
  "main": "release/visualizer",
  "typings": "cjs/statevis.d.ts"
}
